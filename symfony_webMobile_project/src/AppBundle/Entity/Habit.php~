<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * Created by PhpStorm.
 * User: nida ilyas
 * Date: 07/10/2016
 * Time: 15:18
 */

/**
 * Klant
 *
 * @ORM\Table(name="habits")
 * @ORM\Entity
 */
class Habit
{
    /**
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\Column(name="description", type="string")
     */
    private $description;
}